{
  "name": "za-express",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "server": "node -r dotenv/config ./server/bin/www",
    "dev": "sh clear_port.sh && NODE_ENV=development npm run server",
    "prod": "NODE_ENV=production npm run server",
    "watch": "nodemon"
  },
  "dependencies": {
    "cookie-parser": "^1.4.4",
    "debug": "^2.6.9",
    "express": "^4.16.1",
    "express-session": "^1.17.1",
    "morgan": "^1.9.1",
    "mysql": "^2.18.1",
    "nodemon": "^2.0.6"
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-security": "^1.4.0",
    "prettier": "^2.2.1"
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "server/*"
    ]
  }
}
