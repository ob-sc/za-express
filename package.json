{
  "name": "za-express",
  "version": "0.1.0",
  "engines": {
    "node": ">=8.4.0"
  },
  "scripts": {
    "start": "npm run prod",
    "watch": "npm run dev",
    "server": "node -r dotenv/config index.js",
    "dev": "NODE_ENV=development pm2-dev start ecosystem.config.js --env development",
    "prod": "NODE_ENV=production pm2 ecosystem.config.js --env production",
    "lint": "eslint .",
    "dcup": "docker-compose up -d",
    "dcdown": "docker-compose down"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "debug": "^2.6.9",
    "dotenv": "^9.0.0",
    "esm": "^3.2.25",
    "express": "^4.16.1",
    "express-mysql-session": "^2.1.5",
    "express-session": "^1.17.1",
    "joi": "^17.4.0",
    "morgan": "^1.9.1",
    "mysql": "^2.18.1",
    "nodemailer": "^6.6.0"
  },
  "devDependencies": {
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-security": "^1.4.0",
    "prettier": "^2.2.1"
  }
}
